@model object

<div class="form-group">
    <div class="col-sm-4 text-right">
        @Html.LabelFor(model => model, new { @class = "control-label" })
        @if (ViewData["required"] != null)
        {
            <span class="required"> *</span>
        }
    </div>
    @*@Html.LabelFor(model => model, new { @class = "col-sm-4 control-label" })*@
    <div class="col-sm-7">

        @Html.DropDownListFor(model => model, (SelectList)ViewData["selectList"], new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model, "", new { @class = "text-danger" })

    </div>
</div>
