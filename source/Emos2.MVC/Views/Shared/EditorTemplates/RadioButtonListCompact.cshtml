@model object

    @foreach (var li in (IList<SelectListItem>)ViewData["selectList"])
    {

        <div class="checkbox checkbox-right">
            <label>
                @li.Text
                @Html.RadioButtonFor(model => model, li.Value, Model.ToString() == li.Value ? new { @checked = "checked" } : null)
                <span class="span-radio"></span>
            </label>
        </div>
        
    }


    @Html.ValidationMessageFor(model => model, "", new { @class = "text-danger" })


